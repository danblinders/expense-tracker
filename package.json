{
  "name": "expense-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "firebase": "^9.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "indent": [
        1,
        2
      ],
      "semi": [
        1,
        "always"
      ],
      "quotes": [
        1,
        "single"
      ],
      "jsx-quotes": [
        1,
        "prefer-double"
      ],
      "comma-style": [
        1,
        "last"
      ],
      "no-trailing-spaces": [
        1,
        {
          "skipBlankLines": true
        }
      ],
      "no-multiple-empty-lines": [
        1,
        {
          "max": 1,
          "maxBOF": 1,
          "maxEOF": 0
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
